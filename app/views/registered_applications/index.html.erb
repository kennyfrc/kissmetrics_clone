<h1> All Registered Applications </h1>

<div class = "row">
    <div class = "media-heading">
      <%= link_to "New Application", new_registered_application_path, class: "btn btn-success" %> 
  </div>
  <div class = "col-md-8">
    <% @registered_applications.each do |app| %>
      <div class = "media">
        <div class = "media-body">
          <h4 class = "media-heading">
            <%= link_to app.name, registered_application_path(app.id) %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>

</div>